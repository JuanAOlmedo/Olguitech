<%= link_to contactos_path, class:'information-section-link', method: :get do %>
  <div class='information-section contacto'>
    <h2 class='information-title'> Contacto </h2>
    <div class='information-subsection'>
      <h3 class='information-subtitle'> Contactanos </h3>
    </div>
    <% if user_signed_in? %>
      <p class='information-text'> Clickea en cualquier lugar para contactarse con nosotros </p>
    <% else %>
      <p class='information-text'> Clickea en cualquier lugar para iniciar sesion y contactarse con nosotros </p>
    <% end %>
  </div>
<% end %>